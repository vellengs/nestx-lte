{
    "name": "nestx-lte-cli",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "keywords": [],
    "author": "vellengs",
    "license": "ISC",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "db": "rm -rf entities & npx typeorm-model-generator -h localhost -d models -p 3306 -u root -x Viking\\$123 -e mysql -o src\\/entities --noConfig true --ce pascal --cp camel",
        "start": "nest start",
        "swagger": "swaggerGen -c ./swagger.config.json",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/common": "^7.0.0",
        "@nestjs/core": "^7.0.0",
        "@nestjs/mapped-types": "^0.0.2",
        "@nestjs/mongoose": "^6.4.0",
        "@nestjs/platform-express": "^7.0.0",
        "@nestjs/serve-static": "^2.1.0",
        "@nestjs/swagger": "^4.5.1",
        "@types/lodash": "^4.14.150",
        "class-transformer": "^0.2.3",
        "class-validator": "^0.11.1",
        "dayjs": "^1.8.26",
        "log4js": "^6.2.1",
        "mongoose": "^5.9.10",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.5.4",
        "typeorm": "^0.2.24"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.0.0",
        "@nestjs/schematics": "^7.0.0",
        "@nestjs/testing": "^7.0.0",
        "@types/express": "^4.17.3",
        "@types/jest": "25.1.4",
        "@types/mongoose": "^5.7.14",
        "@types/node": "^13.9.1",
        "@types/supertest": "^2.0.8",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-import": "^2.20.1",
        "handlebars": "^4.7.6",
        "jest": "^25.1.0",
        "prettier": "^1.19.1",
        "supertest": "^4.0.2",
        "swagger-ui-express": "^4.1.4",
        "ts-jest": "25.2.1",
        "ts-json-schema-generator": "^0.67.1",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typeorm-model-generator": "^0.4.2",
        "typescript": "^3.7.4",
        "velocity.js": "^0.3.4",
        "velocityjs": "^2.0.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
